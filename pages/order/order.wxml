<!--index.wxml-->
<view class='pagemain'>

  <view>
    <swiper class='swiperView' indicator-dots='true' autoplay='true' interval='5000' duration='1000' circular='true'>
      <block wx:for="{{imgUrls}}">
        <swiper-item>
          <image class='siwperImg' src='{{item.src}}'></image>
        </swiper-item>
      </block>
    </swiper>
  </view>

</view>